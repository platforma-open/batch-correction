{
  "name": "@platforma-open/milaboratories.batch-correction",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf block-pack/* && block-tools pack"
  },
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@platforma-open/milaboratories.batch-correction.workflow": "workspace:*",
    "@platforma-open/milaboratories.batch-correction.model": "workspace:*",
